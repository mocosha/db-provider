<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Mocosha.DbProvider</AssemblyName>
    <AssemblyTitle>Mocosha.DbProvider</AssemblyTitle>
    <Description>Simple database access library on top of ADO.NET </Description>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <DebugType>portable</DebugType>
    <Company>mocosha Inc.</Company>
    <Version>1.0.1</Version>
    <Authors>mocosha Inc.</Authors>
    <Copyright>mocosha Inc.</Copyright>
    <PackageProjectUrl>https://github.com/mocosha/db-provider</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/mocosha/db-provider/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/mocosha/db-provider.git</RepositoryUrl>
    <PackageOutputPath>./nupkg</PackageOutputPath>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <EnableDefaultCompileItems>true</EnableDefaultCompileItems>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Data.SqlClient" Version="4.5.1" />
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
  </ItemGroup>
</Project>